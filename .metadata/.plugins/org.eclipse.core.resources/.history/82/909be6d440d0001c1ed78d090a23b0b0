package com.Threads;

public class CurrentThreadDemo
public static void main(String args[])
try
Thread tl-Thread.current Thread();
System.out.println ("MainThread ID: " +tl.getId());
Worker w-new Worker ();
w.callme ();
System.out.println("In Main After callme");
} catch (Exception e)
System.out.println (e.getMessage());
}
}
}